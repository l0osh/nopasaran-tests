{
  "id": "EXCHANGE_SYNC",
  "initial": "INITIALIZING",
  "states": {
    "INITIALIZING": {
      "on": {
        "STARTED": [
          {
            "target": "LOADING_TWO_PARAMETERS",
            "actions": [],
            "internal": false
          }
        ]
      }
    },
    "LOADING_TWO_PARAMETERS": {
      "entry": [
        {
          "type": "get_parameters (ctrl msg)"
        },
        {
          "type": "done"
        }
      ],
      "on": {
        "DONE": [
          {
            "target": "SYNC",
            "actions": [
              {
                "type": "assign (ctrl) (ctrl)"
              },
              {
                "type": "assign (msg) (msg)"
              }
            ],
            "internal": false
          }
        ]
      }
    },
    "SYNC": {
      "entry": {
        "type": "sync (ctrl msg)"
      },
      "on": {
        "SYNC_SENT": [
          {
            "target": "WAITING_FOR_SYNC",
            "actions": [
              {
                "type": "assign (ctrl) (ctrl)"
              }
            ],
            "internal": false
          }
        ]
      }
    },
    "WAITING_FOR_SYNC": {
      "entry": [
        {
          "type": "set (10) (timeout)"
        },
        {
          "type": "wait_sync_signal (ctrl timeout) (msg)"
        }
      ],
      "on": {
        "TIMEOUT": [
          {
            "target": "RETURNING_TIMEOUT",
            "actions": []
          }
        ],
        "SYNC_AVAILABLE": [
          {
            "target": "RETURNING_SYNC_MESSAGES",
            "actions": [
              {
                "type": "assign (msg) (msg)"
              }
            ]
          }
        ]
      }
    },
    "RETURNING_TIMEOUT": {
      "entry": [
        {
          "type": "set (TIMEOUT) (event)"
        },
        {
          "type": "set (NULL) (msg)"
        },
        {
          "type": "return_values (event msg)"
        }
      ]
    },
    "RETURNING_SYNC_MESSAGES": {
      "entry": [
        {
          "type": "set (DONE) (event)"
        },
        {
          "type": "return_values (event msg)"
        }
      ]
    }
  }
}
